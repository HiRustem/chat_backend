generator client {
    provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model User {
    id Int @id @default(autoincrement())
    username String @unique
    password String 
    name String
    avatar String
    chats String[]
    key Int
}

model Chat {
    id Int @id @default(autoincrement())
    name String
    avatar String
    messages Json[]
    members String[]
}